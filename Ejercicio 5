public class Main {
    public static void main(String[] args) {
        // Crear un arreglo de enteros
        int[] numeros = {1, 2, 3, 4, 5};

        // Iterar sobre el arreglo usando foreach
        for (int numero : numeros) {
            System.out.println("NÃºmero: " + numero);
        }
    }
}
