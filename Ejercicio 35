import java.util.Scanner;
public class SDFSDFSD   {
	 public enum DiasSemana {
	        LUNES,
	        MARTES,
	        MIERCOLES,
	        JUEVES,
	        VIERNES,
	        SABADO,
	        DOMINGO;
	    }
	  public static void main(String[] args) {
	        
	        Scanner sn = new Scanner(System.in);
	        System.out.println("Escribe un dia de la semana");
	        String dia = sn.next();
	        
	        DiasSemana caca = DiasSemana.valueOf(dia.toUpperCase());
	        
	        switch(caca){
	            case LUNES:
	            case MARTES:
	            case MIERCOLES:
	            case JUEVES:
	            case VIERNES:
	                System.out.println("El dia "+caca.name().toLowerCase()+" es laborable");
	                break;
	            case SABADO:
	            case DOMINGO:
	                System.out.println("El dia "+caca+" no es laborable");
	                break;
	            
	        }
	        
	    }
	    
	}
